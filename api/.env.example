# Copy to .env and update with your values

# Authentication - API Keys (comma-separated)
VALID_API_KEYS=prod-api-key-123,backup-key-456,client-key-789

# JWT Secret (make this long and random in production)
JWT_SECRET_KEY=super-secret-jwt-signing-key-make-it-very-long-and-random

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# AWS Configuration (optional, uses defaults)
AWS_REGION=us-east-1
STAGE=dev

# Custom Domain (optional)
DOMAIN_NAME=api.yourdomain.com
CERTIFICATE_NAME=*.yourdomain.com